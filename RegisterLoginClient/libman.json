{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@6.5.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "axios@1.7.2",
      "destination": "wwwroot/axios/"
    },
    {
      "library": "moment.js@2.30.1",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "library": "vue@3.4.33",
      "destination": "wwwroot/lib/vue/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@8.0.7",
      "destination": "wwwroot/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}